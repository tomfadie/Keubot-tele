{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "handler": "telegram_webhook"
      }
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "main.py:telegram_webhook",
      "methods": ["POST"]
    }
  ]

}

